// Se o valor da mesada for maior ou igual a R$50,00, vamos ao cinema. 
// Menor que R$ 40,00 e maior ou igual a R$ 30,00, vamos comprar pipoca e Netflix. 
// Menor que R$30,00, vamos apenas assistir Netflix.

let mesada
mesada = parseFloat(window.prompt("Digite o valor da sua mesada: "))

if (mesada >= 50) {
console.log("Vamos ao cinema!")
}
else if (mesada >= 40 && mesada <= 49) {
console.log("Vamos comprar pipoca e assistir Netflix.")
}
else if (mesada < 40 && mesada >= 30) {
console.log("Vamos comprar pipoca e assistir Netflix.")
}
else {
console.log("Vamos apenas assistir Netflix.")
}
