let nota1, nota2, nota3, nota4
let media

nota1 = parseFloat(window.prompt("Digite a sua primeira nota: "))
nota2 = parseFloat(window.prompt("Digite a sua segunda nota: "))
nota3 = parseFloat(window.prompt("Digite a sua terceira nota: "))
nota4 = parseFloat(window.prompt("Digite a sua quarta nota: "))
media = (nota1 + nota2 + nota3 + nota4)/4

console.log("A sua média é de: " + media)
